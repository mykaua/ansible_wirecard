[Interface]
Address = {{ item.item.value.wg_private_ip }}/24
PrivateKey = {{ item['content'] | b64decode | replace('\n', '')}}
DNS = {{ client_dns_1 }},{{client_dns_2}}


[Peer]
PublicKey = {{ user_publickey['content'] | b64decode | replace('\n', '')}}
PresharedKey = {{ user_presharedkey['content'] | b64decode | replace('\n', '')}}
Endpoint = {{ public_ip }}:{{wg_port}}
AllowedIPs = {{ allowed_ips }}
